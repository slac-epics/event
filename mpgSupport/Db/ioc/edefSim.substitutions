	  { IOC:SYS1:1 ,  ""   , "IOC:SYS1:1:PNET" , "IOC:SYS1:1:EDEFFO" ,  ""   }
}
file mpgPatternTest.db {
  pattern { SYSI  , UNITI }
 	  { SYS1  ,  1    }
}

