	  {$(SYSI),  7,   7   ,   ""     , "" ,  1 ,  0 ,  0,  0  , $(DEVICE) }
	  {$(SYSI),  8,   8   ,   ""     , "" ,  1 ,  0 ,  0,  0  , $(DEVICE) }
	  {$(SYSI),  9,   9   ,   ""     , "" ,  1 ,  0 ,  0,  0  , $(DEVICE) }
	  {$(SYSI), 10,  10   ,   ""     , "" ,  1 ,  0 ,  0,  0  , $(DEVICE) }
	  {$(SYSI), 11,  11   ,   ""     , "" ,  1 ,  0 ,  0,  0  , $(DEVICE) }
	  {$(SYSI), 12,  12   ,   ""     , "" ,  1 ,  0 ,  0,  0  , $(DEVICE) }
	  {$(SYSI), 13,  13   ,   ""     , "" ,  1 ,  0 ,  0,  0  , $(DEVICE) }
	  {$(SYSI), 14,  14   ,   ""     , "" ,  1 ,  0 ,  0,  0  , $(DEVICE) }
	  {$(SYSI), 15,  15   ,   ""     , "" ,  1 ,  0 ,  0,  0  , $(DEVICE) }
# Next 4 measurement definitions for 1Hz, 10Hz, and Full 
	  {$(SYSI), 16,  1HZ  , "1HZ"    ,SYS ,  1 ,  1 , -1,  0  , $(DEVICE) }
	  {$(SYSI), 17, 10HZ  ,"10HZ"    ,SYS ,  1 ,  1 , -1,  0  , $(DEVICE) }
	  {$(SYSI), 18, FULL  , "FULL"   ,SYS ,  1 ,  1 , -1,  0  , $(DEVICE) }
# Last 2 measurement definitions for fast feedback
	  {$(SYSI), 19,$(F1ID), $(FBCK1) ,SYS ,  1,   1 , -1,  0  , $(DEVICE) }
	  {$(SYSI), 20,$(F2ID), $(FBCK2) ,SYS ,  1,   1 , -1,  0  , $(DEVICE) }
}
