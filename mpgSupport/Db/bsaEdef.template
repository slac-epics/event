file db/bsaEdef.db
{
#                   Meas Defn                    0=Off
#	         #    ID      Name               1=On 
pattern   {SYS , MD, MDID  ,  NAME    ,USER,NAVG,CTRL,NMEAS, MODE , DEV       }
# First 15 measurement definitions for HLA and CA users
	  {$(SYSI),  1,   1   ,   ""     , "" ,  1 ,  0 ,  0,  0  , $(DEVICE) }
	  {$(SYSI),  2,   2   ,   ""     , "" ,  1 ,  0 ,  0,  0  , $(DEVICE) }
