record(erevent, "$(DEV):$(NAME)CTRL") {
  field(DESC, "$(NAME) Control")
  field(PINI, "YES")
  field(DTYP, "MRF Event Receiver")
  field(OUT, "#C$(CARD) S0 @")
  field(ENAB, "Enabled")
  field(ENM, "$(ID)")
  field(OUT0, "$(P0)")
  field(OUT1, "$(P1)")
  field(OUT2, "$(P2)")
  field(OUT3, "$(P3)")
  field(OUT4, "$(P4)")
  field(OUT5, "$(P5)")
  field(OUT6, "$(P6)")
  field(OUT7, "$(P7)")
  field(OUT8, "$(P8)")
  field(OUT9, "$(P9)")
  field(OUTA, "$(P10)")
  field(OUTB, "$(P11)")
  field(OUTC, "$(P12)")
  field(OUTD, "$(P13)")
  field(VME,  "$(VME)")
  field(SDIS, "$(DEV):$(NAME)CTRL.ENM")
  field(DISV, "1")
  field(DISS, "INVALID")
  info(autosaveFields, "ENM OUT0 OUT1 OUT2 OUT3 OUT4 OUT5 OUT6 OUT7 OUT8 OUT9 OUTA OUTB OUTC OUTD VME ENAB")
}

record(longSub, "$(DEV):$(NAME)LSUBEVDESC") {
  field(DESC, "$(NAME) EV Desc")
  field(PINI, "YES")
  field(SCAN, "Passive")
  field(INAM, "lsubEvDescInit")
  field(SNAM, "lsubEvDesc")
  field(INPA, "$(DEV):$(NAME)CTRL.ENM CPP NMS")
  field(INPB, "$(DEV):$(NAME)DESC.PROC PP NMS")
  field(FLNK, "$(DEV):$(NAME)DESC PP NMS")
}

record(stringin, "$(DEV):$(NAME)DESC") {
  field(SCAN, "Passive")
}
