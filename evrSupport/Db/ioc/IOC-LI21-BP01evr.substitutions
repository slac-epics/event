file db/evrEventRate.db
{
# PRIM = IOC
# UNIT = xxyy, where xx = subsystem ID and yy=index of that subsystem in LOCA.
#
#	 Location Unit   Card        
pattern { LOCA  , UNIT , CARD , NAME    , ID , FLNK                     }
	{ LI21  , BP01 ,   3  , EVENT1  , 1  , "IOC:LI21:BP01:FIDUCIAL" }
	{ LI21  , BP01 ,   3  , EVENT2  , 2  , ""                       }
	{ LI21  , BP01 ,   3  , EVENT3  , 3  , ""                       }
	{ LI21  , BP01 ,   3  , EVENT4  , 4  , ""                       }
	{ LI21  , BP01 ,   3  , EVENT5  , 5  , ""                       }
	{ LI21  , BP01 ,   3  , EVENT6  , 6  , ""                       }
	{ LI21  , BP01 ,   3  , EVENT7  , 7  , ""                       }
	{ LI21  , BP01 ,   3  , EVENT8  , 8  , ""                       }
	{ LI21  , BP01 ,   3  , EVENT9  , 9  , ""                       }
	{ LI21  , BP01 ,   3  , EVENT10 , 10 , ""                       }
	{ LI21  , BP01 ,   3  , EVENT11 , 11 , ""                       }
	{ LI21  , BP01 ,   3  , EVENT12 , 12 , ""                       }
	{ LI21  , BP01 ,   3  , EVENT13 , 13 , ""                       }
	{ LI21  , BP01 ,   3  , EVENT14 , 14 , ""                       }
	{ LI21  , BP01 ,   3  , EVENT15 , 15 , ""                       }
