  pattern { DEVICE      , LOC , UN , SYS }
	  { PMT:XX05:1  , XL05,EV01, SYS0}
	  { PMT:XX05:2  , XL05,EV01, SYS0}
	  { PMT:XX05:3  , XL05,EV01, SYS0}
	  { PMT:XX05:4  , XL05,EV01, SYS0}
	  { PMT:XX05:5  , XL05,EV01, SYS0}
	  { PMT:XX05:6  , XL05,EV01, SYS0}
}
file bsaBLENEdef.db {
  pattern { DEVICE      , LOC , UN , SYS }
	  { BLEN:XX05:1 , XL05,EV01, SYS0}
	  { BLEN:XX05:2 , XL05,EV01, SYS0}
